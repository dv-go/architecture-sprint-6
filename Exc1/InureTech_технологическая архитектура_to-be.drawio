        <mxCell id="HlJHgFVcDTPW6L8RVwm2-11" value="&lt;div style=&quot;font-size: 14px;&quot;&gt;&lt;div style=&quot;text-align: left; font-size: 10px;&quot;&gt;Pods:&lt;/div&gt;&lt;div style=&quot;text-align: left; font-size: 10px;&quot;&gt;- /ready&lt;/div&gt;&lt;div style=&quot;text-align: left; font-size: 10px;&quot;&gt;- /live&lt;/div&gt;&lt;div style=&quot;text-align: left; font-size: 10px;&quot;&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=&quot;text-align: left; font-size: 10px;&quot;&gt;При ошибке запросы перенаправляются на другой под&lt;/div&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="mW3zMy_77x5YiZytnxg1-18">
